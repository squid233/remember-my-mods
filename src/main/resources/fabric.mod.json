  "name": "Remember My Mods",
  "description": "Remember your mods for each save and warns if you removed any mods.",
  "authors": [
    "squid233"
  ],
  "contact": {
    "sources": "https://github.com/squid233/remember-my-mods"
  },
  "license": "MIT",
  "icon": "assets/remember-my-mods/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.squid233.remembermymods.RememberMyMods"
    ]
  },
  "mixins": [
    {
      "config": "remember-my-mods.client.mixins.json",
